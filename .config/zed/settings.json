{
    "telemetry": {
        "diagnostics": false,
        "metrics": false
    },
    "theme": "Zedokai Darker (Filter Spectrum)",
    "icon_theme": "Bearded Icon Theme",
    "title_bar": {
        "show_menus": true
    },
    "tabs": {
        "file_icons": true,
        "git_status": true
    },
    "base_keymap": "VSCode",
    "ui_font_family": ".ZedSans",
    "buffer_line_height": "standard",
    "buffer_font_family": "MesloLGS NF",
    "buffer_font_size": 14.0,
    "terminal": {
        "max_scroll_history_lines": 100000
    },
    "colorize_brackets": true,
    "show_whitespaces": "trailing",
    "languages": {
        "Python": {
            "colorize_brackets": true,
            "enable_language_server": true,
            "format_on_save": "on",
            "show_completions_on_input": true,
            "show_completion_documentation": true
        }
    },
    "wsl_connections": [
        {
            "distro_name": "archlinux",
            "user": null,
            "projects": [
                {
                    "paths": ["/root"]
                }
            ]
        }
    ],
    "disable_ai": true
